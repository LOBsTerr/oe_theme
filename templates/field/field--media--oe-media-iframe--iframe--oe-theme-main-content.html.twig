{#
/**
 * @file
 * Theme override for iframe fields on media items.
 */
#}
{% for item in items %}
  {% if ratio is empty %}
    <div class="media-iframe-wrapper">
      <div class="ecl-media-container__media">{{ item.content }}</div>
    </div>
  {% else %}
    {% include '@ecl-twig/media-container' with {
      'embedded_media': item.content,
      'ratio': ratio|replace({'_': '-'}),
    } %}
  {% endif %}
{% endfor %}
